{
  "name": "react-vectoria",
  "version": "0.1.0",
  "description": "A modern React SVG icon library with CSS and Framer Motion animations",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./animated": {
      "import": {
        "types": "./dist/animated.d.ts",
        "default": "./dist/animated.js"
      },
      "require": {
        "types": "./dist/animated.d.cts",
        "default": "./dist/animated.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "example": "vite example",
    "generate": "tsx scripts/generate.ts",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "framer-motion": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "framer-motion": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "animation",
    "framer-motion",
    "components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axonapiwit/react-vectoria"
  },
  "homepage": "https://github.com/axonapiwit/react-vectoria#readme",
  "bugs": {
    "url": "https://github.com/axonapiwit/react-vectoria/issues"
  },
  "author": "axonapiwit",
  "engines": {
    "node": ">=18"
  }
}
